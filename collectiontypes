// ç”¨æ•°ç»„å­—é¢é‡åˆ›å»ºé›†åˆ
//var favoriteGenres: Set<String> = ["Rock", "Classical", "Hip hop", ]
var favoriteGenres: Set = ["Rock", "Classical", "Hip hop", ]

// count
print("I have \(favoriteGenres.count) favorite music genres.")

// isEmpty
if favoriteGenres.isEmpty {
    print("As far as music goes, I'm not picky.")
} else {
    print("I have particular music preferences.")
}

// insert
favoriteGenres.insert("Jazz")

// remove
if let removedGenre = favoriteGenres.remove("=r") {
    print("\(removedGenre)? I'm over it.")
} else {
    print("I never much cared for that.")
}

// contains
if favoriteGenres.contains("Classical") {
    print("I get up on the good foot.")
} else {
    print("It's too funky in here.")
}

// for in
for genre in favoriteGenres {
    print(genre)
}

// sorted
for genre in favoriteGenres.sorted() {
    print("\(genre)")
}

// é›†åˆæ“ä½œ

// intersection
let oddDigits: Set = [1, 3, 5, 7, 9]
let evenDigits: Set = [0, 2, 4, 6, 8]
let singleDigitPrimeNumbers: Set = [2, 3, 5, 7]

//oddDigits.sorted()
// union åˆå¹¶ï¼Œå»æ‰é‡å¤çš„
oddDigits.union(evenDigits).sorted()
//oddDigits

// intersectionäº¤é›†
oddDigits.intersection(evenDigits).sorted()

// subtracting aé›†åˆä¸åœ¨bé›†åˆä¸­çš„å€¼ç”Ÿæˆé›†åˆ
oddDigits.subtracting(singleDigitPrimeNumbers).sorted()
oddDigits.subtracting(evenDigits).sorted()
evenDigits.subtracting(singleDigitPrimeNumbers).sorted()

// è¡¥é›†ï¼Œäº¤é›†çš„ç›¸å
oddDigits.symmetricDifference(singleDigitPrimeNumbers).sorted()




// é›†åˆæˆå‘˜å…³ç³»å’Œç›¸ç­‰

let houseAnimals: Set = ["ğŸ¶", "ğŸ±"]
let farmAnimals: Set = ["ğŸ®", "ğŸ”", "ğŸ‘", "ğŸ¶", "ğŸ±"]
let cityAnimals: Set = ["ğŸ¦", "ğŸ­"]

// == é›†åˆæ¯”è¾ƒ
houseAnimals == cityAnimals

// isSubset aé›†åˆæ˜¯å¦è¢«bé›†åˆåŒ…å«
houseAnimals.isSubset(of: farmAnimals)

// isSuperset aé›†åˆæ˜¯å¦åŒ…å«bé›†åˆ
farmAnimals.isSuperset(of: houseAnimals)

// isDisjoint æ˜¯å¦æ²¡æœ‰äº¤é›†
farmAnimals.isDisjoint(with: cityAnimals)


// isStrictSubset æ˜¯å¦ä¸ºå­é›†åˆ
houseAnimals.isStrictSubset(of: farmAnimals)

// isStrictSuperset æ˜¯å¦ä¸ºçˆ¶é›†åˆ
farmAnimals.isStrictSuperset(of: houseAnimals)




